@model LogModel

<div class="col-sm-6">
          <h3>Login here!</h3>
          <form action="/login" method="post">
            <div class="form-group">
            <span asp-validation-for="LEmail"></span>
              <label asp-for="LEmail">Email:</label>
              <input
                class="form-control"
                placeholder="example@examp.com"
                asp-for="LEmail"
              />
            </div>
            <div class="form-group">
            <span asp-validation-for="LPassword"></span>
              <label asp-for="LPassword">Password:</label>
              <input
                class="form-control"
                placeholder="password"
                asp-for="LPassword"
              />
            </div>
            <button type="submit" class="btn btn-primary">Log in</button>
          </form>
        </div>
        </div>